<connection-dialog class='rockset'>
  <connection-config>
    <authentication-mode value='Basic' />
    <authentication-options>
      <option name="UsernameAndPassword" default="true" />
    </authentication-options>
    <db-name-prompt value="Database:" default="commons"/>
    <has-databases value="true" />
    <has-schemas value="true" />
    <server-prompt value="Server: " default="api.rs2.usw2.rockset.com"/>
  </connection-config>
</connection-dialog>
